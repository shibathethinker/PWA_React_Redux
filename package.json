{
  "name": "pwa_with_react_reactapp",
  "homepage": "http://shibathethinker.github.io/pwa_with_react_reactapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "http-server": "^0.11.1",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-scripts": "2.0.4"
  },
  "scripts": {
    "start": "http-server -c-1 -p 9000",
    "build": "react-scripts build && sw-precache --config=sw-precache-config.js",
    "test": "react-scripts test  --watch",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
  "deploy": "pwa_with_react_reactapp -d build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "gh-pages": "^2.0.1",
    "jest-enzyme": "^7.0.0",
    "sw-precache": "^5.2.1"
  }
}
